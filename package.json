{
  "name": "sara-query-assistant",
  "version": "1.0.0",
  "description": "SARA - Smart Analytics & Reporting Assistant for business data querying",
  "private": true,
  "keywords": ["analytics", "reporting", "sql", "ai", "dashboard"],
  "author": "Amit Bhattacharya",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "preview": "npm run build && npm run start",
    "clean": "rm -rf .next out dist",
    "db:generate": "prisma generate --schema=prisma/auth-schema.prisma && prisma generate --schema=prisma/business-schema.prisma",
    "db:push:auth": "prisma db push --schema=prisma/auth-schema.prisma",
    "db:push:business": "prisma db push --schema=prisma/business-schema.prisma",
    "db:studio:auth": "prisma studio --schema=prisma/auth-schema.prisma",
    "db:studio:business": "prisma studio --schema=prisma/business-schema.prisma",
    "postinstall": "npm run db:generate"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@prisma/client": "^5.10.0",
    "@radix-ui/react-accordion": "1.2.2",
